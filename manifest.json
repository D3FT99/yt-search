{
    "manifest_version": 3,
    "name": "Youtube right click search",
    "description": "A basic example extension to Youtube right click search",
    "version": "1.0.0",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }, 
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    }
}